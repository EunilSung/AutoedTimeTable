<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.att.autoedtimetable.ui.Home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_best_timetable"
        android:name="com.att.autoedtimetable.ui.BestTimeTable.BestTimeTableFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_best_timetable" />

    <fragment
        android:id="@+id/navigation_my_setting"
        android:name="com.att.autoedtimetable.ui.MySetting.MySettingFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_my_setting" />
